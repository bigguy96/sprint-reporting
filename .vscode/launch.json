{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Reports.Cli",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Reports.Cli/bin/Debug/net9.0/Reports.Cli.dll",
          "args": [
        "--org", "your-org",
        "--project", "YourProject",
        "--team", "YourTeam"
      ],
      "cwd": "${workspaceFolder}/Reports.Cli",
      "console": "integratedTerminal",
      "stopAtEntry": false
    }
  ]
}
